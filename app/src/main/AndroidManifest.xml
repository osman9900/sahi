<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="osman.example.com.sahi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:logo="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Audition.Word_length_2_5.WordLengthActivity"
            android:label="Audition"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Audition.Audition"
            android:label="@string/audition"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Audition.Months_2_5"
            android:label="@string/audition"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Audition.Months_6_9"
            android:label="@string/audition"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Audition.Months_9_18"
            android:label="@string/audition"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Audition.Months_18"
            android:label="@string/audition"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
